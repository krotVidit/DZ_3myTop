import java.util.*;
public class Exercise4 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        System.out.println("Введите число от 1 до 12,которое соответствует месяцу в одно из 4 сезонов года ");
        int month = scan.nextInt();

        switch (month){
            case 1 :
                System.out.println("Вы ввели Январь\nСезон:Зима");
                break;
            case 2 :
                System.out.println("Вы ввели Февраль\nСезон:Зима");
                break;
            case 3 :
                System.out.println("Вы ввели Март\nСезон:Весна");
                break;
            case 4 :
                System.out.println("Вы ввели Апрель\nСезон:Весна");
                break;
            case 5 :
                System.out.println("Вы ввели Май\nСезон:Весна");
                break;
            case 6 :
                System.out.println("Вы ввели Июнь\nCезон:Лето");
                break;
            case 7 :
                System.out.println("Вы ввели Июль\nCезон:Лето");
                break;
            case 8 :
                System.out.println("Вы ввели Август\nCезон:Лето");
                break;
            case 9 :
                System.out.println("Вы ввели Сентябрь\nCезон:Осень");
                break;
            case 10 :
                System.out.println("Вы ввели Октябрь\nCезон:Осень");
                break;
            case 11 :
                System.out.println("Вы ввели Ноябрь\nCезон:Осень");
                break;
            case 12 :
                System.out.println("Вы ввели Декабрь\nСезон:Зима");
                break;
            default:
                System.out.println("Не найден порядковый месяц");

        }
    }
}
